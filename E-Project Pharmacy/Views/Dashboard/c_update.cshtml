@{
    Layout = "Dash";
}
@model E_Project_Pharmacy.Models.categare;

<form asp-action="p_edit" class="depart_form w-50" method="post" enctype="multipart/form-data">
    <div class="w-100 d-flex justify-content-end pr-3 pt-3">
        <i class="croos fa-sharp fa-solid fa-xmark"></i>
    </div>
    <div class="form-group w-75 m-auto pt-4">
        <label for="exampleInputEmail1">Name</label>
        <input type="hidden" asp-for="c_id" asp-controller="Dashboard" />
        <input asp-for="c_name" asp-controller="Dashboard" type="text" class="form-control" aria-describedby="emailHelp"
               placeholder="Enter Departments Name">
    </div>
    <div class="form-group w-75 m-auto pt-4">
        <label for="exampleFormControlFile1">Choose Img</label>
        <input type="file" asp-for="c_img" asp-controller="Dashboard" class="form-control-file">
    </div>
    <div class="btnss w-100 d-flex justify-content-center mb-3 mt-3">
        <button class="btn btn-primary ">Submit</button>
    </div>
</form>


