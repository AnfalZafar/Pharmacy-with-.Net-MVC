                    <td>
                        <p class="fw-normal mb-1"></p>

                    </td>
                    <td>
                        <span class=""></span>
                    </td>
                   
                    <td>
                        <a asp-route-id="" asp-action="" asp-controller="Dashboard" class="btn btn-danger btn-sm btn-rounded">
                            Delete
                        </a>
                    </td>
                    <td>
                        <a asp-route-id="" asp-action="" asp-controller="Dashboard" class="btn btn-primary btn-sm btn-rounded">
                            Edit
                        </a>
                    </td>
                </tr>

            

        </tbody>
    </table>


