@{
    Layout = "Dash";
}
@model E_Project_Pharmacy.Models.orders;

<div class="content">


    <!-- Table Start -->
    <table class="table align-middle mb-0 bg-white">
        <thead class="bg-light">
            <tr>
                <th>id</th>
                <th>Price</th>
                <th>Date Time</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (var value in ViewBag.order)
            {
                <tr>
                    <td>
                        <div class="d-flex align-items-center">
                            
                            <div class="ms-3">
                                <p class="fw-bold mb-1">@value.user_id</p>

                            </div>
                        </div>
                    </td>
                    <td>
                        <p class="fw-normal mb-1">@value.o_price</p>

                    </td>
                    <td>
                        <span class="">@value.DateTime</span>
                    </td>
                    
                </tr>

            }

        </tbody>
    </table>


</div>
