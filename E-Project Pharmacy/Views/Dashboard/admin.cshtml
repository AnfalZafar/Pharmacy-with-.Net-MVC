@{
    Layout = "Dash";
}
@model E_Project_Pharmacy.Models.Users

<form asp-action="adminadd" asp-controller="Dashboard" class="depart_form w-50" method="post" enctype="multipart/form-data">
    <div class="w-100 d-flex justify-content-end pr-3 pt-3">
        <i class="croos fa-sharp fa-solid fa-xmark"></i>
    </div>
    <div class="form-group w-75 m-auto pt-4">
        <label for="exampleInputEmail1">Name</label>
        <input asp-for="user_name" asp-controller="Home" type="text" class="form-control" aria-describedby="emailHelp"
               placeholder="Enter Admin Name">
    </div>
    <div class="form-group w-75 m-auto pt-4">
        <label for="exampleInputEmail1">Email</label>
        <input asp-for="user_email" asp-controller="Home" type="text" class="form-control" aria-describedby="emailHelp"
               placeholder="Enter Admin Email">
    </div>
    <div class="form-group w-75 m-auto pt-4">
        <label for="exampleInputEmail1">Password</label>
        <input asp-for="user_password" asp-controller="Home" type="text" class="form-control" aria-describedby="emailHelp"
               placeholder="Enter Admin Password">
    </div>

    <div class="form-group w-75 m-auto pt-4">
        <label for="exampleInputEmail1">Phone</label>
        <input asp-for="user_phone" asp-controller="Home" type="text" class="form-control" aria-describedby="emailHelp"
               placeholder="Enter Admin Phone">
    </div>
    <div class="btnss w-100 d-flex justify-content-center mb-3 mt-3">
        <button class="btn btn-primary ">Submit</button>
    </div>
</form>

