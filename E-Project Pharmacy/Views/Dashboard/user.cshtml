@{
    Layout = "Dash";
}
@model E_Project_Pharmacy.Models.Users;

<div class="content">


    <!-- Table Start -->
    <table class="table align-middle mb-0 bg-white">
        <thead class="bg-light">
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var value in @ViewBag.user)
            {
                <tr>
                    <td>
                        <div class="d-flex align-items-center">
                           
                            <div class="ms-3">
                                <p class="fw-bold mb-1">@value.user_name</p>

                            </div>
                        </div>
                    </td>
                    <td>
                        <p class="fw-normal mb-1">@value.user_email</p>

                    </td>
                    <td>
                        <span class="">@value.user_phone</span>
                    </td>
                   
                </tr>

            }

        </tbody>
    </table>


</div>
